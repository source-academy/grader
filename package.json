{
  "name": "grader",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "cross-env TIMEOUT=2000 jest --env=node",
    "test-coveralls": "scripts/ci.sh"
  },
  "dependencies": {
    "gpu.js": "2.15.2",
    "js-slang": "^1.0.14",
    "lodash": "4.17.21",
    "node-canvas-webgl": "^0.2.6"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "cross-zip-cli": "^1.0.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^5.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/test.*\\.ts$"
  }
}
