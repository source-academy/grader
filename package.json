{
  "name": "grader",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "cross-env TIMEOUT=2000 jest --env=node",
    "test-coveralls": "scripts/ci.sh"
  },
  "dependencies": {
    "js-slang": "^0.4.63",
    "lodash": "4.17.20",
    "node-canvas-webgl": "^0.2.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cross-zip-cli": "^1.0.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/test.*\\.ts$"
  }
}
