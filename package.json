{
  "name": "grader",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "bash ./scripts/build.sh",
    "typecheck": "tsc --noEmit",
    "test": "cross-env TIMEOUT=2000 jest --env=node",
    "test-coveralls": "cross-env TIMEOUT=2000 jest --env=node --coverage --coverageReporters=text-lcov --forceExit | coveralls"
  },
  "dependencies": {
    "js-slang": "^0.4.57",
    "lodash": "4.17.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cross-zip-cli": "^1.0.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/test.*\\.ts$"
  }
}
